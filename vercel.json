{
  "version": 2,
  "functions": {
    "api/github/webhooks/index.ts": {
      "runtime": "vercel-node@0.12.0"
    }
  },
  "routes": [
    {
      "src": "/api/github/webhooks/(.*)",
      "dest": "/api/github/webhooks/index.ts"
    }
  ],
  "env": {
    "APP_ID": "@app-id",
    "WEBHOOK_SECRET": "@webhook-secret",
    "PRIVATE_KEY": "@private-key",
    "LOG_LEVEL": "debug",
    "DISCORD_WEBHOOK_URL": "@discord-webhook-url"
  }
}
